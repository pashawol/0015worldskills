mixin s-participant()
	// start s-participant
	+b.SECTION.s-participant.section#s-participant&attributes(attributes)
		.container
			.row
				.col.order-lg-first
					+b.section-title
						h2 Участникам
				
				.col-12
					+e.row.row
						mixin item(i)
							+e.col.col-lg.col-md-4.col-6
								+e.img-wrap
									img.res-i.lazy(src="#" data-src=`img/participant-i-${i}.svg`, alt="")
								block
						+item('1')
							+e.title Мастер классы
						+item('2')
							+e.title Диалоги на равных
						+item('3')
							+e.title Нетворкинг
						+item('4')
							+e.title Лекции
						+item('5')
							+e.title Спорт и здоровый образ жизни
						+item('6')
							+e.title Вечерние активности
				.col-lg-auto.order-lg-first
					.row.text-center.justify-content-center
						.col-md-auto
							+e.link(href="#") #молодыепрофессионалы     
						.col-md-auto 
							+e.link(href="#")  #worldskillsRussia
	// end s-participant